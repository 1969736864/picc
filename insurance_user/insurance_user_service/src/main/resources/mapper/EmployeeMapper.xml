<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.java.user.mapper.EmployeeMapper">
    <select id="querybackstageById" resultType="org.java.user.pojo.Employee" >
        select * from employee where
        emp_id=#{id}
    </select>
    <select id="UserbyId" resultType="org.java.user.pojo.Employee" >
        select * from employee where username=#{name}
    </select>

</mapper>